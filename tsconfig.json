{
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "lib",
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": ["typings/modules"]
  },
  "exclude": [
    ".git",
    "node_modules"
  ]
}
